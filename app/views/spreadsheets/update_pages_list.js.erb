$('#paginate-loading-layer').remove();

<% if defined? error %>
  alert("<%= t('.page_not_inported') %> <%= error %>");
<% else %>
  <% if initial_page %>
    $(".initial-page-name")
      .removeClass("no-initial-page")
      .text("<%= initial_page %>");
  <% end %>

  $('.list').empty();
  $("<%= escape_javascript(render 'pages/page') %>")
    .appendTo(".spreadsheet");
<% end%>
